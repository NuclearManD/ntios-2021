{
	"mcu": "IMXRT1062",
	"mcu_ld": "imxrt1062_t41.ld",

	"cc": "arm-none-eabi-gcc",
	"ld": "arm-none-eabi-g++",
	"cxx": "arm-none-eabi-g++",
	"objcopy": "arm-none-eabi-objcopy",
	"objdump": "arm-none-eabi-objdump",
	"size": "arm-none-eabi-size",
	"ar": "arm-none-eabi-gcc-ar",

	"cpu_flags": "-mcpu=cortex-m7 -mfloat-abi=hard -mfpu=fpv5-d16 -mthumb",
	"cpp_flags": "-DUSB_SERIAL_HID -DLAYOUT_US_ENGLISH -DTEENSY41 -g -O3 -I. -ffunction-sections -fdata-sections -Iincludes -fno-builtin-printf",
	"cxx_flags": "-fno-exceptions -Wno-error=narrowing",
	"c_flags": "",
	"ld_flags": "-lm -lstdc++ -Os -Wl,--gc-sections,--relax",

	"executable_type": "ARM32_THUMB"
}
